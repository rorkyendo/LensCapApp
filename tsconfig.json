{
  "extends": [
    "expo/tsconfig.base"
  ],
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "target": "esnext",
    "moduleResolution": "node",
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "app/Index,tsx"
  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
